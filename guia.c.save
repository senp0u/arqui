#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>


void leerArchivo(){
  FILE * fp;
  char * line = NULL;
  size_t len = 0;

  fp = fopen("numbers.txt", "r");
  if (fp == NULL)
     exit(EXIT_FAILURE);
     
   while (getline(&line, &len, fp) != -1) {
"%s", line);
   }

   fclose(fp);
   if (line)
      free(line);
   exit(EXIT_SUCCESS);  
}

int main(void){
    leerArchivo();
}
